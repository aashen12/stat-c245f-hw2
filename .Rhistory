yrange=range(targetmatvec[targetmatvec>0])
ncontrol=str_count(xsg_c[1,1],"/")+1
targetmatvec_log=log2(targetmatvec)
if(ncontrol>1 & nrow(targetmatvec_log)>1){
ctrlmean=rowMeans(targetmatvec_log[,1:ncontrol])
